<div class="photo-shoot">

    <h2 class="scrollmenu_title">PHOTO SHOOT</h2>

    <div class="scrollmenu">
      <% for(image in images) {%>
          <img class="myImg" src=<%= "photo-shoot/"+images[image].imagename %> >
      <% }%>
  </div>
</div>

      <div id="myModal" class="modal">

